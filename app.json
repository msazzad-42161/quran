{
  "expo": {
    "name": "quran",
    "slug": "quran",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/quran.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "assets/*"
    ],
    "splash": {
      "image": "./assets/images/quran.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quran.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.newto944.quran"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
